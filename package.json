{
  "author": "Colten Krauter",
  "bin": {
    "ff": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "colors": "^1.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "memoize-one": "^6.0.0",
    "ora": "^8.0.1"
  },
  "description": "The File Foundry CLI is your ultimate companion for managing and organizing files on your media server.",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/cli-progress": "^3.11.5",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.1.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/coltenkrauter/file-foundry-cli",
  "license": "MIT",
  "main": "",
  "name": "file-foundry-cli",
  "oclif": {
    "bin": "ffc",
    "dirname": "ffc",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "coltenkrauter/file-foundry-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "dev": "npm run build && node ./bin/run"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/coltenkrauter/file-foundry-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
